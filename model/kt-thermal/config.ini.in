[common]
bootstrap=1
initial_turnoff=1
first_pwr_status_on=1
time_hold_power=15
time_hold_ign=10
log_poweroff=0
sms_passwd=etrace
alive_time_sec=15

[gps]
gps_time_zone=9
adjust_gps=1
gps_err_enable=1
gps_err_speed_kms=200
gps_err_dist_kms=150
gps_err_ignore_dist_m=1500
gps_err_first_dist_m=2000
gps_err_max_dist_m=12000

[mileage]
enable=1
daily_mileage=0

[webdm]
enable=1
tx_power=1
tx_ignition=1
tx_report=0
tx_log=1

[noti]
turn_on_sms=0
target_num=01089619571

[model]
tempature_enable = 1
tempature_device=/dev/ttyHSL2
tempature_cycle = 60

[dist_gps_filter]
comment = "filter above filter value (unit:meter)"
enable = 1
value = 50

[sat_gps_filter]
comment = "filter under filter value"
enable = 0
value = 5

[hdop_gps_filter]
comment = "filter above filter value"
enable = 0
value = 5

[speed_gps_filter]
comment = "filter above filter value (unit:Km)"
enable = 1
value = 200
